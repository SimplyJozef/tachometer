const int epd_bitmap_allArray_LEN = 1;
const unsigned char* epd_bitmap_allArray[1] = {
	epd_bitmap_INTAKE_H
};


#include <TFT_eSPI.h>

TFT_eSPI tft = TFT_eSPI();

int prev_rectWidth = 0;

void setup() {
  Serial.begin(115200);
  
  tft.init();
  tft.setRotation(1);
  tft.fillScreen(TFT_WHITE);
}

void loop() {
  for (int intake_temp = 0; intake_temp <= 45; intake_temp += 1) {
    drawIntakeTemp(0, 0, intake_temp);
    delay(200);
  }
}

void drawIntakeTemp(int x, int y, int intake_temp) {
  tft.drawBitmap(x, y, epd_bitmap_allArray[0], 320, 80, TFT_BLACK);

  int rectWidth = map(intake_temp, 0, 45, 0, 320);

  if (rectWidth != prev_rectWidth) {
    tft.fillRect(x + prev_rectWidth, y + 14, 320 - prev_rectWidth, 66, TFT_WHITE);
    tft.fillRect(x, y + 14, rectWidth, 66, TFT_BLACK);
    prev_rectWidth = rectWidth;
  }

  tft.setTextColor(TFT_BLACK, TFT_WHITE);
  tft.setTextSize(2);
  tft.fillRect(289, y + 2, 50, 20, TFT_WHITE);
  tft.setCursor(289, y + 2);
  tft.print(intake_temp);

  if (intake_temp >= 45) {
    intake_temp = 0;
    prev_rectWidth = 0;
    tft.fillRect(x, y + 14, 320, 66, TFT_WHITE);
  }
}

void drawIntakeTempGraphHorizont(int x, int y, int intake_temp) {
  tft.drawBitmap(x, y, epd_bitmap_allArray[0], 320, 80, TFT_BLACK);
  int rectHight = map(intake_temp, 0, 45, x+66, y+14);
}