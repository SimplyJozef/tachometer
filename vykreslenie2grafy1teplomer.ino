#include <TFT_eSPI.h>

TFT_eSPI tft = TFT_eSPI();

int prev_rectWidth = 0;

const int graphWidth = 320;
const int graphHeight = 66;
const int barWidth = 1;

int barHeights1[graphWidth];
int barHeights2[graphWidth];

uint16_t buffer1[graphWidth * graphHeight];
uint16_t buffer2[graphWidth * graphHeight];

void setup() {
  Serial.begin(115200);

  tft.init();
  tft.setRotation(1);
  tft.fillScreen(TFT_WHITE);

  for (int i = 0; i < graphWidth; i++) {
    barHeights1[i] = 0;
    barHeights2[i] = 0;
  }

  for (int i = 0; i < graphWidth * graphHeight; i++) {
    buffer1[i] = TFT_WHITE;
    buffer2[i] = TFT_WHITE;
  }
}

void loop() {
  for (int value = 0; value < 41; value += 1) {
    drawIntakeTemp(160, numbers1[value], 1);
    updateAndDrawGraph(14, numbers2[value], 2, barHeights1, buffer1);
    updateAndDrawGraph(94, numbers3[value], 3, barHeights2, buffer2);
    delay(200);
  }
}

void drawIntakeTemp(int y, int intake_temp, int header) {
  tft.drawBitmap(0, y, epd_bitmap_VEHICLESPEED_H, 320, 14, TFT_BLACK);
  int rectWidth = map(intake_temp, 0, 45, 0, 320);

  if (rectWidth != prev_rectWidth) {
    tft.fillRect(0 + prev_rectWidth, y + 14, 320 - prev_rectWidth, 66, TFT_WHITE);
    tft.fillRect(0, y + 14, rectWidth, 66, TFT_BLACK);
    prev_rectWidth = rectWidth;
  }

  tft.setTextColor(TFT_BLACK, TFT_WHITE);
  tft.setTextSize(2);
  tft.fillRect(289, y + 2, 50, 20, TFT_WHITE);
  tft.setCursor(289, y + 2);
  tft.print(intake_temp);
}

void updateAndDrawGraph(int yOffset, int value, int header, int barHeights[], uint16_t buffer[]) {
  tft.drawBitmap(0, yOffset-14, epd_bitmap_allArray[header], 320, 14, TFT_BLACK);

  for (int i = 0; i < graphWidth - 1; i++) {
    barHeights[i] = barHeights[i + 1];
  }

  int newBarHeight = map(value, 0, 45, 0, graphHeight);
  barHeights[graphWidth - 1] = newBarHeight;

  for (int i = 0; i < graphWidth * graphHeight; i++) {
    buffer[i] = TFT_WHITE;
  }

  for (int x = 0; x < graphWidth; x++) {
    int barHeight = barHeights[x];
    if (barHeight > 0) {
      for (int y = graphHeight - barHeight; y < graphHeight; y++) {
        buffer[y * graphWidth + x] = TFT_BLACK;
      }
    }
  }

  tft.pushImage(0, yOffset, graphWidth, graphHeight, buffer);

  tft.setTextColor(TFT_BLACK, TFT_WHITE);
  tft.setTextSize(2);
  tft.fillRect(289, yOffset-12, 50, 20, TFT_WHITE);
  tft.setCursor(289, yOffset-12);
  tft.print(value);
}


